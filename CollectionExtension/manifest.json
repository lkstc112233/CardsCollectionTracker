{
    "name": "Collection Extension",
    "description": "Manages the collection and stuff",
    "version": "0.6",
    "options_page": "options.html",
    "permissions": [
        "storage"
    ],
    "host_permissions": [
        "https://www.mtggoldfish.com/"
    ],
    "content_scripts": [
        {
            "matches": ["*://www.mtggoldfish.com/*"],
            "js": ["content_script.js"]
        }
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "manifest_version": 3
}